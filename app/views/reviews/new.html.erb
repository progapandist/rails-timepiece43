
<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= simple_form_for [@booking, @review] do |f| %>
        <%= f.input :title %>
         <%= f.input :content %>
         <%= f.input :rating %>
        <%= f.button :submit, class: "btn btn-primary"  %>
      <% end %>
    </div>
  </div>
</div>
